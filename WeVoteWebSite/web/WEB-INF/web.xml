<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>ShowMenu</servlet-name>
        <servlet-class>org.wevote.server.ShowMenuImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowChart</servlet-name>
        <servlet-class>org.wevote.server.ShowChartImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MySQL</servlet-name>
        <servlet-class>org.wevote.server.MySQL</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ShowMenu</servlet-name>
        <url-pattern>/org.wevote.Main/showmenu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowChart</servlet-name>
        <url-pattern>/org.wevote.Main/showchart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MySQL</servlet-name>
        <url-pattern>/MySQL</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
